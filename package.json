{
  "name": "themoviedb-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "setEnv": "ts-node ./src/assets/scripts/setEnv.ts",
    "start": "npm run setEnv -- --environment=dev && ng serve",
    "build": "npm run setEnv -- --environment=prod && ng build",
    "test": "npm run setEnv -- --environment=dev && ng test --watch false --browsers ChromeHeadless --code-coverage",
    "lint": "ng lint",
    "start:pwa": "ng build --prod && http-server -p 8080 -c-1 dist/themoviedb-angular"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.6",
    "@angular/common": "~11.2.6",
    "@angular/compiler": "~11.2.6",
    "@angular/core": "~11.2.6",
    "@angular/forms": "~11.2.6",
    "@angular/platform-browser": "~11.2.6",
    "@angular/platform-browser-dynamic": "~11.2.6",
    "@angular/router": "~11.2.6",
    "@angular/service-worker": "~11.2.6",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.5",
    "@angular/cli": "~11.2.5",
    "@angular/compiler-cli": "~11.2.6",
    "@angular/pwa": "^12.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.20.11",
    "codelyzer": "^6.0.0",
    "dotenv": "^9.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-spec-reporter": "^0.0.32",
    "puppeteer": "^9.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
